@{
    ViewData["Title"] = "Home Page";
}

<head>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
</head>

<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <div class="item active">
            <img src="~/images/JHA1170x400trans.png" class="img-responsive" /><br>
            <div class="carousel-caption" role="option">
                <p>
                    JHA provides more than 300 products and services that enable our customers to process financial transactions, automate their businesses, and succeed in an increasingly competitive marketplace.
                    <a class="btn btn-primary btn3d" href="https://www.jackhenry.com/pages/default.aspx">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/symirat1170x400trans.png" alt="Visual Studio" class="img-responsive" /><br>
            <div class="carousel-caption" role="option">
                <p>
                    Symitar, a division of Jack Henry & Associates.
                    <a class="btn btn-primary btn3d" href="https://www.symitar.com/pages/default.aspx">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/profitstars1170x400trans.png" alt="Package Management" class="img-responsive" /><br>
            <div class="carousel-caption" role="option">
                <p>
                    ProfitStars is a leading software, solution, and technology innovator.
                    <a class="btn btn-primary btn3d" href="https://www.profitstars.com/pages/default.aspx">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
        <div class="item">
            <img src="~/images/vsg1170x400trans.jpg" alt="Microsoft Azure" class="img-responsive" /><br>
            <div class="carousel-caption" role="option">
                <p>
                    Vanguard Software Group is now a part of ProfitStars®, a Jack Henry company.
                    <a class="btn btn-primary btn3d" href="http://www.vsgsolutions.com">
                        Learn More
                    </a>
                </p>
            </div>
        </div>
    </div>
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>

